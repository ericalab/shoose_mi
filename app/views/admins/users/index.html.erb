<div class="table-responsive col-md-9 ml-sm-auto col-lg-10 px-md-4">
  <table class="table table-striped table-sm">
    <h2>受注状況</h2>
    <thead>
      <tr>
        <th>ユーザーID</th>
        <th>ユーザー名</th>
        <th>メールアドレス</th>
        <th>住所</th>
      </tr>
    </thead>
    <tbody>
      <％ @users.each do |@user| %>
        <tr>
          <td><%= @user.id %></td>
          <td><%= @user.name %></td>
          <td><%= @user.email %></td>
          <td>住所</td>
        </tr>
      <% end %>
      <%= link_to "編集", "#" %>
      <%= link_to "削除", "#" %>
    </tbody>
  </table>
</div>